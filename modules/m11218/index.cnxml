	  <row>
	    <entry>
	      <m:math>
		<m:ci>J</m:ci>
	      </m:math>-Divergence
	    </entry>
	    
	    <entry>
	      <m:math>
		<m:apply>
		  <m:times/>
		  <m:apply>
		    <m:minus/>
		    <m:ci><m:mfenced>
			<m:mi>·</m:mi>
		      </m:mfenced></m:ci>
		    <m:cn>1</m:cn>
		  </m:apply>
		  <m:apply>
		    <m:log/>
		    <m:ci><m:mfenced>
			<m:mi>·</m:mi>
		      </m:mfenced></m:ci>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:leq/>
		  <m:apply>
		    <m:times/>
		    <m:ci><m:msub>
			<m:mi>π</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		    <m:ci><m:msub>
			<m:mi>π</m:mi>
			<m:mn>1</m:mn>
		      </m:msub></m:ci>
		    <m:apply>
		      <m:exp/>
		      <m:apply>
			<m:minus/>
			<m:apply>
			  <m:divide/>
			  <m:apply>
			    <m:ci type="fn">d</m:ci>
			    <m:ci><m:msub>
				<m:mi>p</m:mi>
				<m:mn>0</m:mn>
			      </m:msub></m:ci>
			    <m:ci><m:msub>
