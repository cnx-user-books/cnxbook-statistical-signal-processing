			</m:apply>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:ci><m:msub>
		      <m:mi>P</m:mi>
		      <m:mi>e</m:mi>
		    </m:msub></m:ci>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <m:ci type="fn">J</m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
