		    <m:mi>P</m:mi>
		    <m:mi>M</m:mi>
		  </m:msub></m:ci>
	      </m:math> (<m:math>
		<m:ci><m:msub>
		    <m:mi>P</m:mi>
		    <m:mi>F</m:mi>
		  </m:msub></m:ci>
	      </m:math>)
	    </entry>
	  </row>

	  <row>
	    <entry>Kullback-Leibler
	      <m:math>
		<m:apply>
		  <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#distance"/>
		  <m:ci><m:msub>
		      <m:mi>p</m:mi>
		      <m:mn>0</m:mn>
		    </m:msub></m:ci>
		  <m:ci><m:msub>
		      <m:mi>p</m:mi>
		      <m:mn>1</m:mn>
		    </m:msub></m:ci>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:minus/>
		  <m:apply>
		    <m:log/>
		    <m:ci><m:mfenced>
			<m:mi>·</m:mi>
		      </m:mfenced></m:ci>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <m:limit/>
		    <m:bvar>
		      <m:ci>L</m:ci>
		    </m:bvar>
		    <m:lowlimit>
		      <m:infinity/>
		    </m:lowlimit>
		    <m:apply>
		      <m:minus/>
		      <m:apply>
			<m:times/>
			<m:apply>
			  <m:divide/>
			  <m:cn>1</m:cn>
			  <m:ci>L</m:ci>
			</m:apply>
			<m:apply>
			  <m:log/>
			  <m:ci><m:msub>
			      <m:mi>P</m:mi>
			      <m:mi>M</m:mi>
			    </m:msub></m:ci>
			</m:apply>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:apply>
		    <m:ci type="fn">d</m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
