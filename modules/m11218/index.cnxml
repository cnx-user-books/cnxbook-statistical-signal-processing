	  </row>

	  <row>
	    <entry>Chernoff</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:power/>
		  <m:ci><m:mfenced>
		      <m:mi>·</m:mi>
		    </m:mfenced></m:ci>
		  <m:ci>s</m:ci>
		</m:apply>
	      </m:math>, 
		
	      <m:math>
		<m:apply>
		  <m:in/>
		  <m:ci>s</m:ci>
		  <m:interval closure="open">
		    <m:cn>0</m:cn>
		    <m:cn>1</m:cn>
		  </m:interval>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <!-- max of one element?? -->
		    <m:max/>
		    <m:apply>
		      <m:limit/>
		      <m:bvar>
			<m:ci>L</m:ci>
		      </m:bvar>
		      <m:lowlimit>
			<m:infinity/>
		      </m:lowlimit>
		      <m:apply>
			<m:minus/>
			<m:apply>
			  <m:times/>
			  <m:apply>
			    <m:divide/>
			    <m:cn>1</m:cn>
			    <m:ci>L</m:ci>
			  </m:apply>
			  <m:apply>
			    <m:log/>
			    <m:ci><m:msub>
				<m:mi>P</m:mi>
				<m:mi>e</m:mi>
			      </m:msub></m:ci>
			  </m:apply>
			</m:apply>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:apply>
		    <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#infimum"/>
		    <m:bvar>
		      <m:ci>s</m:ci>
		    </m:bvar>
		    <m:condition>
		      <m:apply>
			<m:in/>
			<m:ci>s</m:ci>
			<m:interval closure="open">
			  <m:cn>0</m:cn>
			  <m:cn>1</m:cn>
			</m:interval>
		      </m:apply>
		    </m:condition>
		    <m:apply>
		      <m:ci type="fn">d</m:ci>
		      <m:ci><m:msub>
			  <m:mi>p</m:mi>
			  <m:mn>0</m:mn>
			</m:msub></m:ci>
		      <m:ci><m:msub>
			  <m:mi>p</m:mi>
			  <m:mn>1</m:mn>
			</m:msub></m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>Independent of <foreign>a priori</foreign>
	    probabilities</entry>
	  </row>

	  <row>
	    <entry>
	      <m:math>
		<m:ci>M</m:ci>
	      </m:math>-Hypothesis Chernoff</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:power/>
		  <m:ci><m:mfenced>
		      <m:mi>·</m:mi>
		    </m:mfenced></m:ci>
		  <m:ci>s</m:ci>
		</m:apply>
	      </m:math>, 
		
	      <m:math>
		<m:apply>
		  <m:in/>
		  <m:ci>s</m:ci>
		  <m:interval closure="open">
		    <m:cn>0</m:cn>
		    <m:cn>1</m:cn>
		  </m:interval>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <!-- max of one element?? -->
		    <m:max/>
		    <m:apply>
		      <m:limit/>
		      <m:bvar>
			<m:ci>L</m:ci>
		      </m:bvar>
		      <m:lowlimit>
			<m:infinity/>
		      </m:lowlimit>
		      <m:apply>
			<m:minus/>
			<m:apply>
			  <m:times/>
			  <m:apply>
			    <m:divide/>
			    <m:cn>1</m:cn>
			    <m:ci>L</m:ci>
			  </m:apply>
			  <m:apply>
			    <m:log/>
			    <m:ci><m:msub>
				<m:mi>P</m:mi>
				<m:mi>e</m:mi>
			      </m:msub></m:ci>
			  </m:apply>
			</m:apply>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:apply>
		    <m:min/>
		    <!-- needs bvar-->
		    <m:condition>
		      <m:apply>
			<m:neq/>
			<m:ci>i</m:ci>
			<m:ci>j</m:ci>
		      </m:apply>
		    </m:condition>
		    <m:apply>
		      <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#infimum"/>
		      <m:bvar>
			<m:ci>s</m:ci>
		      </m:bvar>
		      <m:condition>
			<m:apply>
			  <m:in/>
			  <m:ci>s</m:ci>
			  <m:interval closure="open">
			    <m:cn>0</m:cn>
			    <m:cn>1</m:cn>
			  </m:interval>
			</m:apply>
		      </m:condition>
		      <m:apply>
			<m:ci type="fn">d</m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mi>i</m:mi>
			  </m:msub></m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mi>j</m:mi>
			  </m:msub></m:ci>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>
	  </row>

	  <row>
	    <entry>Bhattacharyya</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:power/>
		  <m:ci><m:mfenced>
		      <m:mi>·</m:mi>
		    </m:mfenced></m:ci>
		  <m:apply>
		    <m:divide/>
		    <m:cn>1</m:cn>
		    <m:cn>2</m:cn>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:leq/>
		  <m:apply>
		    <m:times/>
		    <m:ci><m:msub>
			<m:mi>π</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		    <m:ci><m:msub>
			<m:mi>π</m:mi>
			<m:mn>1</m:mn>
		      </m:msub></m:ci>
		    <m:apply>
		      <m:power/>
		      <m:apply>
			<m:ci type="fn">d</m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>0</m:mn>
			  </m:msub></m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub></m:ci>
		      </m:apply>
		      <m:cn>2</m:cn>
		    </m:apply>
		  </m:apply>
		  <m:ci><m:msub>
		      <m:mi>P</m:mi>
		      <m:mi>e</m:mi>
		    </m:msub></m:ci>
		  <m:apply>
		    <m:times/>
		    <m:apply>
		      <m:root/>
		      <m:apply>
			<m:times/>
			<m:ci><m:msub>
			    <m:mi>π</m:mi>
			    <m:mn>0</m:mn>
			  </m:msub></m:ci>
			<m:ci><m:msub>
			    <m:mi>π</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub></m:ci>
		      </m:apply>
		    </m:apply>
		    <m:apply>
		      <m:ci type="fn">d</m:ci>
		      <m:ci><m:msub>
			  <m:mi>p</m:mi>
			  <m:mn>0</m:mn>
			</m:msub></m:ci>
		      <m:ci><m:msub>
			  <m:mi>p</m:mi>
			  <m:mn>1</m:mn>
			</m:msub></m:ci>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>Minimizing 
	      <m:math>
		<m:apply>
		  <m:ci type="fn">d</m:ci>
		  <m:ci><m:msub>
		      <m:mi>p</m:mi>
		      <m:mn>0</m:mn>
		    </m:msub></m:ci>
		  <m:ci><m:msub>
		      <m:mi>p</m:mi>
		      <m:mn>1</m:mn>
		    </m:msub></m:ci>
		</m:apply>
	      </m:math> will tend to minimize
	      <m:math>
		<m:ci><m:msub>
		    <m:mi>P</m:mi>
		    <m:mi>e</m:mi>
		  </m:msub></m:ci>
	      </m:math>
	    </entry>
	  </row>

	  <row>
	    <entry>Orsak</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:abs/>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:ci type="fn">
			<m:msub>
			  <m:mi>π</m:mi>
			  <m:mn>1</m:mn>
			</m:msub>
		      </m:ci>
		      <m:ci>·</m:ci>
		    </m:apply>
		    <m:ci><m:msub>
			<m:mi>π</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:ci><m:msub>
		      <m:mi>P</m:mi>
		      <m:mi>e</m:mi>
		    </m:msub></m:ci>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:divide/>
		      <m:cn>1</m:cn>
		      <m:cn>2</m:cn>
		    </m:apply>
		    <m:apply>
		      <m:times/>
		      <m:apply>
			<m:divide/>
			<m:cn>1</m:cn>
			<m:cn>2</m:cn>
		      </m:apply>
		      <m:apply>
			<m:ci type="fn">d</m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>0</m:mn>
			  </m:msub></m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub></m:ci>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>Exact formula for average error probability</entry>
	  </row>

	  <row>
	    <entry>Kolmogorov</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:times/>
		  <m:apply>
		    <m:divide/>
		    <m:cn>1</m:cn>
		    <m:cn>2</m:cn>
		  </m:apply>
		  <m:apply>
		    <m:abs/>
		    <m:apply>
		      <m:minus/>
		      <m:ci><m:mfenced>
			  <m:mi>·</m:mi>
			</m:mfenced></m:ci>
		      <m:cn>1</m:cn>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>

	    <entry>If
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:ci><m:msub>
		      <m:mi>π</m:mi>
		      <m:mn>0</m:mn>
		    </m:msub></m:ci>
		  <m:ci><m:msub>
		      <m:mi>π</m:mi>
		      <m:mn>1</m:mn>
		    </m:msub></m:ci>
		</m:apply>
	      </m:math>,
	      <m:math>
		<m:apply>
		  <m:eq/>
		  <m:ci><m:msub>
		      <m:mi>P</m:mi>
		      <m:mi>e</m:mi>
		    </m:msub></m:ci>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:divide/>
		      <m:cn>1</m:cn>
		      <m:cn>2</m:cn>
		    </m:apply>
		    <m:apply>
		      <m:times/>
		      <m:apply>
			<m:divide/>
			<m:cn>1</m:cn>
			<m:cn>2</m:cn>
		      </m:apply>
		      <m:apply>
			<m:ci type="fn">d</m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>0</m:mn>
			  </m:msub></m:ci>
			<m:ci><m:msub>
			    <m:mi>p</m:mi>
			    <m:mn>1</m:mn>
			  </m:msub></m:ci>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		</m:apply>
	      </m:math>
	    </entry>
	  </row>

	  <row>
	    <entry>Hellinger</entry>

	    <entry>
	      <m:math>
		<m:apply>
		  <m:power/>
		  <m:apply>
		    <m:minus/>
		    <m:apply>
		      <m:power/>
		      <m:ci><m:mfenced>
			  <m:mi>·</m:mi>
			</m:mfenced></m:ci>
		      <m:apply>
			<m:divide/>
			<m:cn>1</m:cn>
			<m:cn>2</m:cn>
		      </m:apply>
		    </m:apply>
		    <m:cn>1</m:cn>
		  </m:apply>
		  <m:cn>2</m:cn>
		</m:apply>
	      </m:math>
	    </entry>
	  </row>

	</tbody>
      </tgroup>
    </table>

  </content>
  
