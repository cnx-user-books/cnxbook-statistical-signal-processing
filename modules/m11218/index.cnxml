		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:apply>
		    <m:ci type="fn">d</m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
			<m:mn>0</m:mn>
		      </m:msub></m:ci>
		    <m:ci><m:msub>
			<m:mi>p</m:mi>
