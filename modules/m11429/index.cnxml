	      </m:math> or 
	      <m:math display="block">
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <m:int/>
		    <m:bvar>
		      <m:ci>x</m:ci>
		    </m:bvar>
		    <m:apply>
		      <m:plus/>
		      <m:apply>
			<m:times/>
			<m:apply>
			  <m:partialdiff/>
