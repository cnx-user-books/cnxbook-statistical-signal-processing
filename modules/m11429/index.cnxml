			  <m:log/>
			  <m:apply>
			    <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#pdf">p</m:csymbol>
			    <m:condition>
			      <m:ci>θ</m:ci>
			    </m:condition>
			    <m:ci>x</m:ci>
			  </m:apply>
			</m:apply>
		      </m:apply>
		      <m:apply>
			<m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#pdf">p</m:csymbol>
			<m:condition>
			  <m:ci>θ</m:ci>
			</m:condition>
			<m:ci>x</m:ci>
		      </m:apply>
		    </m:apply>
		  </m:apply>
		  <m:cn>0</m:cn>
		</m:apply>
	      </m:math>
	      Thus,
	      <m:math display="block">
		<m:apply>
		  <m:eq/>
		  <m:apply>
		    <m:partialdiff/>
		    <m:bvar>
		      <m:ci>θ</m:ci>
		    </m:bvar>
