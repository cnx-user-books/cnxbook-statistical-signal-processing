			  <m:apply>
			    <m:log/>
			    <m:apply>
			      <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#pdf">p</m:csymbol>
			      <m:condition>
				<m:ci>θ</m:ci>
