			      <m:ci>θ</m:ci>
			    </m:condition>
			    <m:ci>x</m:ci>
			  </m:apply>
			</m:apply>
		      </m:apply>
		      <m:apply>
			<m:partialdiff/>
			<m:bvar>
			  <m:ci>θ</m:ci>
			</m:bvar>
			<m:apply>
			  <m:log/>
			  <m:apply>
			    <m:csymbol definitionURL="http://cnx.rice.edu/cd/cnxmath.ocd#pdf">p</m:csymbol>
			    <m:condition>
			      <m:ci>θ</m:ci>
			    </m:condition>
